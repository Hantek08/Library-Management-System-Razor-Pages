@page
@model AgilSystemutveckling_Xamarin_Net5.Pages.TestPages.TestPageCreateUserModel

<form method="post">
    <div class="border p-3 mt-4">
        <div class="row pb-2">
            <h2 class="text-primary pl-3">Create User</h2>
            <hr />
        </div>
        <!-- <div class="mb-3">
           <label asp-for="Model.Author.Id"></label>
            <input asp-for="Model.Author.Id" class="form-control" />
        </div> -->
        <div class="mb-3">
           <label asp-for="@Model.user.Username"></label>
            <input asp-for="@Model.user.Username" class="form-control" />
        </div>
         <div class="mb-3">
           <label asp-for="@Model.user.Password"></label>
            <input asp-for="@Model.user.Password" class="form-control" />
        </div>
         <div class="mb-3">
           <label asp-for="@Model.user.Level"></label>
            <input asp-for="@Model.user.Level" class="form-control" />
        </div>
         <div class="mb-3">
           <label asp-for="@Model.user.Address"></label>
            <input asp-for="@Model.user.Address" class="form-control" />
        </div>
         
        <button type="submit" class="btn btn-primary" style="width:150px;">Create</button>
    </div>
</form>